<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>requestApiServlet</servlet-name>
        <servlet-class>com.yakumo.servlet.RequestApiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>requestApiServlet</servlet-name>
        <url-pattern>/requestApiServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>parameterServlet</servlet-name>
        <servlet-class>com.yakumo.servlet.ParameterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>parameterServlet</servlet-name>
        <url-pattern>/parameterServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>servlet01</servlet-name>
        <servlet-class>com.yakumo.servlet.forward.Servlet01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet01</servlet-name>
        <url-pattern>/servlet01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>servlet02</servlet-name>
        <servlet-class>com.yakumo.servlet.forward.Servlet02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet02</servlet-name>
        <url-pattern>/servlet02</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>forwardC</servlet-name>
        <servlet-class>com.yakumo.servlet.forward.ForwardC</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>forwardC</servlet-name>
        <url-pattern>/forwardC</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>responseIOServlet</servlet-name>
        <servlet-class>com.yakumo.servlet.ResponseIOServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>responseIOServlet</servlet-name>
        <url-pattern>/responseIOServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>response01</servlet-name>
        <servlet-class>com.yakumo.servlet.redirect.Response01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>response01</servlet-name>
        <url-pattern>/response01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>response02</servlet-name>
        <servlet-class>com.yakumo.servlet.redirect.Response02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>response02</servlet-name>
        <url-pattern>/response02</url-pattern>
    </servlet-mapping>
</web-app>